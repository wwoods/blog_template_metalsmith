{
  "name": "blog_template_metalsmith",
  "description": "Example toolbox using metalsmith to keep a worklog",
  "version": "1.0.0",
  "author": {
    "name": "Walt Woods",
    "email": "waltlgp@gmail.com"
  },
  "license": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc && node build/index.js build",
    "debug-incremental": "tsc && echo 'for use with chrome://inspect' && node --inspect-brk build/index.js serve --incremental",
    "start": "tsc && node build/index.js serve",
    "start-nonlocal": "tsc && node build/index.js serve --non-local",
    "start-incremental": "tsc && node build/index.js serve --incremental"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "highlight.js": "^10.1.2",
    "jstransformer-scss": "^1.0.0",
    "metalsmith": "^2.3.0",
    "metalsmith-dom-transform": "^2.0.0",
    "metalsmith-sass": "^1.6.0",
    "multimatch": "^2.1.0",
    "node-natural-sort": "^0.8.6",
    "pug": "^2.0.4",
    "typescript": "^2.5.3",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/multimatch": "^2.1.2",
    "@types/node": "^8.0.47",
    "@types/pug": "^2.0.4",
    "@types/xml2json": "^0.10.0",
    "http-server": "^0.10.0",
    "metalsmith-watch": "^1.0.3"
  }
}
